{
  "name": "apollo-sentry-helper",
  "version": "1.1.1",
  "description": "Capture your Apollo GraphQL exceptions into Sentry, in a meaningful way.",
  "author": "Pedro Bini @ Jungsoft",
  "license": "MIT",
  "repository": "jungsoft/apollo-sentry-helper",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint ./src --ext .js,.ts",
    "prebuild": "yarn run lint",
    "build": "rollup -c",
    "copy_publish_files": "cp ../README.md .",
    "prepublish": "yarn run build && yarn run copy_publish_files"
  },
  "dependencies": {
    "@sentry/minimal": "^5.20.0",
    "apollo-link-error": "^1.1.13",
    "merge": "^1.2.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "gh-pages": "^3.1.0",
    "graphql": "^15.3.0",
    "rollup": "^2.23.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "rollup-plugin-url": "^3.0.1",
    "scheduler": "0.19.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "apollo",
    "sentry"
  ]
}
